algoritmo "Votação"

var
voto: literal

inicio

Escreval("Digite seu voto:")
leia(voto)

 se voto = "13" entao
    Escreval("Você votou no Lulinha")
 senao
 se voto = "22" entao
    Escreval("Você votou no bozo")
 se voto = "12" entao
    Escreval("VocÊ votou no Cirão da massa")
 fimse
 fimse
 fimse

fimalgoritmo
