algoritmo "Votação - Caso"

var
voto: literal

inicio

Escreval("Digite seu voto:")
leia(voto)

Escolha voto
caso "13"
     Escreval("Você votou no lulinha")
caso "22"
     Escreval("Você votou no bozo")
caso "12"
     Escreval("VocÊ votou no cirão da massa")
outrocaso
     Escreval("Voto nulo")
fimescolha

fimalgoritmo
